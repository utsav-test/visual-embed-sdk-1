{
    "name": "@thoughtspot/visual-embed-sdk",
    "version": "1.2.0-alpha.6",
    "description": "ThoughtSpot Embed SDK",
    "module": "lib/src/index.js",
    "main": "dist/tsembed.js",
    "types": "lib/src/index.d.ts",
    "files": [
        "dist/**",
        "lib/**",
        "src/**"
    ],
    "exports": {
        ".": "./lib/src/index.js",
        "./react": "./src/react/index.js"
    },
    "scripts": {
        "lint": "eslint 'src/**'",
        "lint:fix": "eslint 'src/**/*.*' --fix",
        "tsc": "tsc -p . --incremental false",
        "start": "gatsby develop",
        "build:gatsby": "npm run clean:gatsby && gatsby build --prefix-paths",
        "build:gatsby:noprefix": "npm run clean:gatsby && gatsby build",
        "serve:gatsby": "gatsby serve",
        "clean:gatsby": "gatsby clean",
        "build-and-publish": "npm run build:gatsby && npm run publish",
        "bundle-dts": "dts-bundle --name @thoughtspot/visual-embed-sdk --out visual-embed-sdk.d.ts --main lib/src/index.d.ts",
        "build": "rollup -c",
        "watch": "rollup -cw",
        "docs-cmd": "node scripts/gatsby-commands.js",
        "docgen": "typedoc --tsconfig tsconfig.json --theme typedoc-theme",
        "test-sdk": "jest -c jest.config.sdk.js",
        "test": "npm run test-sdk",
        "posttest": "cat ./coverage/sdk/lcov.info | coveralls",
        "prepublishOnly": "npm run test; npm run tsc; npm run bundle-dts; npm run build",
        "publish-dev": "npm publish --tag dev",
        "publish-prod": "npm publish --tag prod"
    },
    "peerDependencies": {
        "react": "> 16.8.0",
        "react-dom": "> 16.8.0"
    },
    "dependencies": {
        "gatsby": "3.1.0",
        "mixpanel-browser": "^2.41.0"
    },
    "devDependencies": {
        "@mdx-js/mdx": "^1.6.22",
        "@mdx-js/react": "^1.6.22",
        "@react-icons/all-files": "^4.1.0",
        "@rollup/plugin-commonjs": "^18.0.0",
        "@rollup/plugin-node-resolve": "^11.2.1",
        "@testing-library/dom": "^7.31.0",
        "@testing-library/jest-dom": "^5.12.0",
        "@testing-library/react": "^11.2.6",
        "@testing-library/user-event": "^13.1.8",
        "@types/jest": "22.2.3",
        "@types/mixpanel-browser": "^2.35.6",
        "@typescript-eslint/eslint-plugin": "^4.6.0",
        "@typescript-eslint/parser": "^4.6.0",
        "asciidoctor": "^2.2.1",
        "command-line-args": "^5.1.1",
        "coveralls": "^3.1.0",
        "dts-bundle": "0.7.3",
        "eslint": "^7.12.1",
        "eslint-plugin-react-hooks": "^4.2.0",
        "eslint-config-airbnb-base": "^14.2.0",
        "eslint-config-prettier": "^6.15.0",
        "eslint-import-resolver-typescript": "^2.3.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-prettier": "^3.1.4",
        "gatsby-plugin-catch-links": "^3.1.0",
        "gatsby-plugin-intl": "^0.3.3",
        "gatsby-plugin-local-search": "^2.0.0",
        "gatsby-plugin-manifest": "^3.2.0",
        "gatsby-plugin-sass": "4.1.0",
        "gatsby-source-filesystem": "3.1.0",
        "gatsby-transformer-asciidoc": "2.1.0",
        "gatsby-transformer-rehype": "2.0.0",
        "gh-pages": "^3.1.0",
        "highlight.js": "^10.6.0",
        "html-to-text": "^7.0.0",
        "jest": "26.4.2",
        "jest-puppeteer": "^4.4.0",
        "node-sass": "^4.0.0",
        "prettier": "2.1.2",
        "puppeteer": "^7.0.1",
        "react": "^16.14.0",
        "react-dom": "^16.14.0",
        "react-resizable": "^1.11.0",
        "react-resize-detector": "^6.6.0",
        "react-use-flexsearch": "^0.1.1",
        "rollup": "2.30.0",
        "rollup-plugin-typescript2": "0.27.3",
        "ts-jest": "26.4.4",
        "ts-loader": "8.0.4",
        "typedoc": "0.20.25",
        "typedoc-neo-theme": "^1.1.0",
        "typedoc-plugin-toc-group": "0.0.5",
        "typescript": "^4.2.4",
        "url-search-params-polyfill": "^8.1.0"
    },
    "author": "ThoughtSpot",
    "email": "support@thoughtspot.com",
    "license": "ThoughtSpot Development Tools End User License Agreement",
    "directories": {
        "lib": "lib"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/thoughtspot/visual-embed-sdk.git"
    },
    "keywords": [
        "thoughtspot",
        "everywhere",
        "embed",
        "sdk",
        "analytics"
    ],
    "bugs": {
        "url": "https://github.com/thoughtspot/visual-embed-sdk/issues"
    },
    "homepage": "https://github.com/thoughtspot/visual-embed-sdk#readme"
}
